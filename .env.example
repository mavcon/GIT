# API Configuration
REACT_APP_API_URL=http://localhost:3000/api
REACT_APP_API_VERSION=v1

# Authentication
REACT_APP_AUTH_DOMAIN=your-auth-domain
REACT_APP_AUTH_CLIENT_ID=your-client-id
REACT_APP_AUTH_AUDIENCE=your-audience

# Google Maps
REACT_APP_GOOGLE_MAPS_KEY=your-google-maps-api-key
REACT_APP_DEFAULT_MAP_CENTER_LAT=43.6532
REACT_APP_DEFAULT_MAP_CENTER_LNG=-79.3832
REACT_APP_DEFAULT_MAP_ZOOM=12

# Storage
REACT_APP_STORAGE_PREFIX=dojo_libre
REACT_APP_STORAGE_VERSION=1.0

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_CHAT=true
REACT_APP_ENABLE_GEOLOCATION=true

# Image Upload
REACT_APP_MAX_IMAGE_SIZE=5242880
REACT_APP_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/gif
REACT_APP_DEFAULT_AVATAR_URL=/default-avatar.png

# Rate Limiting
REACT_APP_API_RATE_LIMIT=100
REACT_APP_API_RATE_WINDOW=60000

# Timeouts (in milliseconds)
REACT_APP_API_TIMEOUT=5000
REACT_APP_GEOLOCATION_TIMEOUT=10000
REACT_APP_CHAT_POLL_INTERVAL=3000

# Development Tools
REACT_APP_ENABLE_REDUX_DEVTOOLS=true
REACT_APP_ENABLE_QUERY_DEVTOOLS=true
REACT_APP_ENABLE_MOCK_API=true

# Social Media Integration
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id
REACT_APP_TWITTER_API_KEY=your-twitter-api-key

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=your-ga-id
REACT_APP_ENABLE_ERROR_REPORTING=true

# Cache Configuration
REACT_APP_CACHE_TTL=3600000
REACT_APP_MAX_CACHE_SIZE=100

# Member Settings
REACT_APP_MAX_TRAINING_ARTS=5
REACT_APP_MIN_PASSWORD_LENGTH=8
REACT_APP_MAX_BIO_LENGTH=500

# Dojo Settings
REACT_APP_MAX_DOJO_IMAGES=10
REACT_APP_MAX_DOJO_CAPACITY=100
REACT_APP_MIN_OPERATING_HOURS_INTERVAL=30

# Comments and explanations:
# - All environment variables are prefixed with REACT_APP_ to be accessible in React
# - API_URL: Base URL for backend API requests
# - AUTH_*: Authentication configuration for secure access
# - GOOGLE_MAPS_*: Configuration for map functionality
# - STORAGE_*: Local storage configuration
# - Feature flags enable/disable specific functionality
# - Image upload settings define size and type restrictions
# - Rate limiting helps prevent API abuse
# - Timeouts ensure graceful handling of slow operations
# - Development tools can be enabled/disabled as needed
# - Social media integration for sharing and authentication
# - Analytics track usage and errors
# - Cache settings optimize performance
# - Member and Dojo settings define business rules
